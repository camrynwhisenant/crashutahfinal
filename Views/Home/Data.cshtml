    @*Table to display all accidents*@

    <div class="">
        <div class="table-wrapper card-shadow">
            <div class="table-row header">
                <div class="table-heading">Crash ID</div>
                <div class="table-heading">Date</div>
                <div class="table-heading">City</div>
                <div class="table-heading">County</div>
                <div class="table-heading">Crash Severity</div>
                <div class="table-heading">Actions</div>
            </div>
            <div class="table-container">
                <div class="table-data-wrapper">
                    <div class="scroll-container">
                        <div class="scroll-table-content">

                            @foreach (var a in Model.Accidents)
                            {
                <div class="table-row">
                    <div class="table-box">
                        <div fs-cmssort-field="name" class="table-data name">@a.crash_id</div>
                    </div>
                    
                    @if (a.crash_datetime.Length >= 10)
                    {
                <div class="table-data">@a.crash_datetime.Substring(0, 10)</div>
                }
                else
                {
                <div class="table-data">@a.crash_datetime</div>
                }
                    <div class="table-data">@a.city</div>
                    <div fs-cmsfilter-type="date" fs-cmsfilter-field="date" class="table-data date">@a.county_name</div>
                    <div fs-cmsfilter-type="date" fs-cmsfilter-field="date" class="table-data date">@a.crash_severity_id</div>
                    <div class="div-block-421">
                        <a asp-action="Crash" asp-controller="Home" asp-route-id="@a.crash_id" class="button-secondary-icon-copy w-button"><span class="">More Details</span></a>
                    </div>
                    <div style="display:flex;">
                        @if (this.User.Identity.IsAuthenticated)
                        {
                    <a asp-controller="Home" asp-action="Edit" asp-route-crash_id="@a.crash_id" class="button-secondary-icon-copy w-button">Edit</a>}
                        @if (this.User.Identity.IsAuthenticated)
                        {
                    <a asp-controller="Home" asp-action="Delete" asp-route-crash_id="@a.crash_id" class="button-secondary-icon-copy w-button">Delete</a>}
                    </div>
                    </div>}
                        </div>
                    </div>
                </div>
            </div>
        </div>
        @*Pagination tag helpers*@
        <div style="display:flex; align-content:center">
            <div page-blah="@Model.PageInfo" page-action="Data"
                 page-class="btn" page-class-normal="btn-outline-dark"
                 page-class-selected="btn-primary" class="btn-group pull-right m-1"
                 page-classes-enabled="true">
            </div>
        </div>
    </div>

</div>
    
