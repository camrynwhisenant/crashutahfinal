@model CrashData



    <head>
        <title>Prediction</title>
    </head>

@*Form to allow user to enter inputs and check severity*@

<div class="card-shadow grid">
    <div>
        <h1>Please include all neccessary information</h1>
    </div>
    <form method="post">
        <label asp-for="work_zone_related_True">Work Zone Related:</label>
        <select asp-for="work_zone_related_True">
            <option value="0.0">No</option>
            <option value="1.0">Yes</option>
        </select>
        <br />

        <label asp-for="pedestrian_involved_True">Pedestrian Involved:</label>
        <select asp-for="pedestrian_involved_True">
            <option value="0.0">No</option>
            <option value="1.0">Yes</option>
        </select>
        <br />

        <label asp-for="bicyclist_involved_True">Bicylist Involved:</label>
        <select asp-for="bicyclist_involved_True">
            <option value="0.0">No</option>
            <option value="1.0">Yes</option>
        </select>
        <br />

        <label asp-for="motorcycle_involved_True">Motorcyle Involved:</label>
        <select asp-for="motorcycle_involved_True">
            <option value="0.0">No</option>
            <option value="1.0">Yes</option>
        </select>
        <br />


        <label asp-for="improper_restraint_True">Not properly wearing Seatbelt:</label>
        <select asp-for="improper_restraint_True">
            <option value="0.0">Properly Buckled</option>
            <option value="1.0">Improperly Buckled</option>
        </select>
        <br />


        <label asp-for="unrestrained_True">Wearing Seatbelt:</label>
        <select asp-for="unrestrained_True">
            <option value="0.0">Yes</option>
            <option value="1.0">No</option>
        </select>
        <br />

        <label asp-for="dui_True">Substances involved:</label>
        <select asp-for="dui_True">
            <option value="0.0">No</option>
            <option value="1.0">Yes</option>
        </select>
        <br />


        <label asp-for="intersection_related_True">Crash at intersection:</label>
        <select asp-for="intersection_related_True">
            <option value="0.0">No</option>
            <option value="1.0">Yes</option>
        </select>
        <br />


        <label asp-for="wild_animal_related_True">Wild Animal:</label>
        <select asp-for="wild_animal_related_True">
            <option value="0.0">No</option>
            <option value="1.0">Yes</option>
        </select>
        <br />

        <label asp-for="overturn_rollover_True">Overturn rollover:</label>
        <select asp-for="overturn_rollover_True">
            <option value="0.0">No</option>
            <option value="1.0">Yes</option>
        </select>
        <br />

        <label asp-for="commercial_motor_veh_involved_True">commercial vehicle involved:</label>
        <select asp-for="commercial_motor_veh_involved_True">
            <option value="0.0">No</option>
            <option value="1.0">Yes</option>
        </select>
        <br />

        <label asp-for="teenage_driver_involved_True">Teenage Driver:</label>
        <select asp-for="teenage_driver_involved_True">
            <option value="0.0">No</option>
            <option value="1.0">Yes</option>
        </select>
        <br />


        <label asp-for="older_driver_involved_True">Older Driver:</label>
        <select asp-for="older_driver_involved_True">
            <option value="0.0">No</option>
            <option value="1.0">Yes</option>
        </select>
        <br />


        <label asp-for="single_vehicle_True">Single Vehicle:</label>
        <select asp-for="single_vehicle_True">
            <option value="0.0">No</option>
            <option value="1.0">Yes</option>
        </select>
        <br />


        <label asp-for="distracted_driving_True">Distracted Driving:</label>
        <select asp-for="distracted_driving_True">
            <option value="0.0">No</option>
            <option value="1.0">Yes</option>
        </select>
        <br />

        <label asp-for="drowsy_driving_True">Drowsy Driving:</label>
        <select asp-for="drowsy_driving_True">
            <option value="0.0">No</option>
            <option value="1.0">Yes</option>
        </select>
        <br />

        <label>Day of the Week:</label>
        <select>
            <option asp-for="day_Monday" value="1.0">Monday</option>
            <option asp-for="day_Tuesday" value="1.0">Tuesday</option>
            <option asp-for="day_Saturday" value="1.0">Saturday</option>
            <option asp-for="day_Sunday" value="1.0">Sunday</option>

        </select>
        <br />

        @*<label asp-for="day_Monday">Monday:</label>
            <select asp-for="day_Monday">
                <option value="0.0">No</option>
