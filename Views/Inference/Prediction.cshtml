            </select>
            <br />

            <label asp-for="roadway_departure_True">Roadway Departure:</label>
            <select asp-for="roadway_departure_True">
                <option value="0.0">No</option>
                <option value="1.0">Yes</option>
            </select>
            <br />*@

        <label asp-for="route_68">Route:</label>
        <select asp-for="route_68">
            <option value="0.0">Other</option>
            <option value="1.0">Route 68</option>
        </select>
        <br />

        <label asp-for="county_name_WASATCH">County:</label>
        <select asp-for="county_name_WASATCH">
            <option value="1.0">Wasatch</option>
            <option value="0.0">Other</option>
        </select>
        <br />

        <label asp-for="time_frame_5am_9am">Time of day:</label>
        <select asp-for="time_frame_5am_9am">
            <option value="1.0">5am-9am</option>
            <option value="0.0">9am-12pm</option>
        </select>
        <br />

        @*<label asp-for="time_frame_9am_12pm">Time 9 to 12:</label>
            <select asp-for="time_frame_9am_12pm">
                <option value="0.0">No</option>
                <option value="1.0">Yes</option>
            </select>
